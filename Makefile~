calculadora_git.out: aritmetica.o constantes.o series.o calculadra_git.o
	gcc -o calculadora_git.out constantes.o seies.o aritmetica.o calculadra_git.o -lm
aritmetica.o: aritmetica.c
	gcc -c aritmetica.c
constantes.o: constantes.c
	gcc -c constantes.c
series.o: series.c
	gcc -c series.c
calculadora.o: calculadora_git.c
	gcc -c calculadora_git.c
